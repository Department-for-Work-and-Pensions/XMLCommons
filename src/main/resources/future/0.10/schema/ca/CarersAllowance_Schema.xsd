<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.govtalk.gov.uk/dwp/carers-allowance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
           targetNamespace="http://www.govtalk.gov.uk/dwp/carers-allowance" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.4" id="dwp-ca">
    <xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="schema/ca/xmldsig-core-schema.xsd"/>
    <xs:element name="DWPBody">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Version" type="xs:decimal">
                    <xs:annotation>
                        <xs:documentation>Used to handle several versions of the service. Will be useful during testing phases and later if we open the
                            service.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="DWPCATransaction">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="TransactionId">
                                <xs:annotation>
                                    <xs:documentation>We should probably increase the size since we want to keep them and use them for data
                                        analysis.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[0-9]{2}[0-1][0-9][2-9A-Z]{7}"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="DateTimeGenerated" type="DateTimeType">
                                <xs:annotation>
                                    <xs:documentation>Can be used when generating PDF and for investigations</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="LanguageUsed">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="English"/>
                                        <xs:enumeration value="Welsh"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:choice>
                                <xs:element name="DWPCAClaim">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="DateOfClaim" type="QuestionDateType"/>
                                            <xs:element minOccurs="0" name="QualifyingBenefit" type="QuestionTenTextType"/>
                                            <xs:element name="Claimant">
                                                <xs:complexType>
                                                    <xs:complexContent>
                                                        <xs:extension base="ClaimantPersonType">
                                                            <xs:sequence>
                                                                <xs:element minOccurs="0" name="MaritalStatus" nillable="false" type="QuestionTextType"/>
                                                                <xs:element minOccurs="0" name="TextPhoneContact" type="QuestionTextType"/>
                                                            </xs:sequence>
                                                        </xs:extension>
                                                    </xs:complexContent>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element name="Caree">
                                                <xs:complexType>
                                                    <xs:complexContent>
                                                        <xs:extension base="PersonType">
                                                            <xs:sequence>
                                                                <xs:element minOccurs="0" name="RelationToClaimant" type="QuestionOtherType"/>
                                                                <xs:element minOccurs="0" name="Cared35Hours" type="QuestionTextType"/>
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="CareBreak" type="CareBreakType"/>
                                                                <xs:element minOccurs="0" name="Cared35HoursBefore" type="QuestionTextType"/>
                                                                <xs:element minOccurs="0" name="DateStartCaring" type="QuestionDateType"/>
                                                                <xs:element minOccurs="0" name="LiveSameAddress" type="QuestionTextType"/>
                                                            </xs:sequence>
                                                        </xs:extension>
                                                    </xs:complexContent>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="Residency">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="Nationality" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="ActualNationality" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="NormallyLiveInGB" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="CountryNormallyLive" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" maxOccurs="unbounded" name="PeriodAbroad">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element minOccurs="0" name="TimeOutsideGBLast3Years" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="TripsDetails" type="QuestionTextThreeThousandType"/>
                                                                    <xs:element minOccurs="0" name="Period" type="PeriodType"/>
                                                                    <xs:element minOccurs="0" name="Reason" type="QuestionOtherType"/>
                                                                    <xs:element minOccurs="0" name="Country" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="CareePresent" type="QuestionTextType"/>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="CourseOfEducation" type="QuestionTextType"/>
                                            <xs:element minOccurs="0" name="FullTimeEducation">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="CourseDetails">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element minOccurs="0" name="Type" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="Title" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="DateStarted" type="QuestionDateType"/>
                                                                    <xs:element minOccurs="0" name="DateStopped" type="QuestionDateType"/>
                                                                    <xs:element minOccurs="0" name="ExpectedEndDate" type="QuestionDateType"/>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                        <xs:element minOccurs="0" name="LocationDetails">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element minOccurs="0" name="Name" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="Address" type="QuestionAddressType"/>
                                                                    <xs:element minOccurs="0" name="PhoneNumber" type="QuestionPhoneNumberType"/>
                                                                    <xs:element minOccurs="0" name="FaxNumber" type="QuestionPhoneNumberType"/>
                                                                    <xs:element minOccurs="0" name="StudentReferenceNumber" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="Tutor" type="QuestionTextType"/>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="SelfEmployed" type="QuestionTextType"/>
                                            <xs:element minOccurs="0" name="SelfEmployment">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="SelfEmployedNow" type="QuestionTextType"/>
                                                        <xs:choice>
                                                            <xs:element minOccurs="0" name="RecentJobDetails">
                                                                <xs:complexType>
                                                                    <xs:complexContent>
                                                                        <xs:extension base="TradingType">
                                                                            <xs:sequence>
                                                                                <xs:element minOccurs="0" name="DateEnded" type="QuestionDateType"/>
                                                                                <xs:element minOccurs="0" name="TradingCeased" type="QuestionTextType"/>
                                                                            </xs:sequence>
                                                                        </xs:extension>
                                                                    </xs:complexContent>
                                                                </xs:complexType>
                                                            </xs:element>
                                                            <xs:element minOccurs="0" name="CurrentJobDetails" type="TradingType"/>
                                                        </xs:choice>
                                                        <xs:element minOccurs="0" name="PaidForPension" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="PensionExpenses">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element minOccurs="0" name="Expense" type="QuestionTextThreeHundredType"/>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                        <xs:element minOccurs="0" name="PaidForJobExpenses" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="JobExpenses">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element minOccurs="0" name="Expense" type="QuestionTextThreeHundredType"/>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="Employed" type="QuestionTextType"/>
                                            <xs:element minOccurs="0" name="Employment">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" maxOccurs="5" name="JobDetails">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element minOccurs="0" name="Employer">
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element minOccurs="0" name="CurrentlyEmployed" type="QuestionTextType"/>
                                                                                <xs:element minOccurs="0" name="DidJobStartBeforeClaimDate"
                                                                                            type="QuestionTextType"/>
                                                                                <xs:element minOccurs="0" name="DateJobStarted" type="QuestionDateType"/>
                                                                                <xs:element minOccurs="0" name="DateJobEnded" type="QuestionDateType"/>
                                                                                <xs:element minOccurs="0" name="JobType" type="QuestionTextType"/>
                                                                                <xs:element minOccurs="0" name="Name" type="QuestionTextType"/>
                                                                                <xs:element minOccurs="0" name="Address" type="QuestionAddressType"/>
                                                                                <xs:element minOccurs="0" name="EmployersPhoneNumber"
                                                                                            type="QuestionPhoneNumberType"/>
                                                                                <xs:element minOccurs="0" name="EmployersFaxNumber"
                                                                                            type="QuestionPhoneNumberType"/>
                                                                                <xs:element minOccurs="0" name="HaveFinishedJob" type="QuestionTextType"/>
                                                                                <xs:element minOccurs="0" name="P45LeavingDate" type="QuestionDateType"/>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element minOccurs="0" name="Pay">
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element minOccurs="0" name="WeeklyHoursWorked">
                                                                                    <xs:complexType>
                                                                                        <xs:complexContent>
                                                                                            <xs:extension base="QuestionType">
                                                                                                <xs:sequence>
                                                                                                    <xs:element name="Answer" type="NumberHoursType"/>
                                                                                                </xs:sequence>
                                                                                            </xs:extension>
                                                                                        </xs:complexContent>
                                                                                    </xs:complexType>
                                                                                </xs:element>
                                                                                <xs:element minOccurs="0" name="DateLastPaid" type="QuestionDateType"/>
                                                                                <xs:element minOccurs="0" name="GrossPayment" type="QuestionMoneyType"> </xs:element>
                                                                                <xs:element minOccurs="0" name="IncludedInWage" type="QuestionTextThreeHundredType"/>
                                                                                <xs:element minOccurs="0" name="PayFrequency" type="QuestionOtherType"/>
                                                                                <xs:element minOccurs="0" name="UsualPayDay" type="QuestionTextType"/>
                                                                                <xs:element minOccurs="0" name="ConstantEarnings" type="QuestionTextType"/>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element minOccurs="0" name="OweMoney" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="PaidForPension" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="PensionExpenses">
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element minOccurs="0" name="Expense" type="QuestionTextThreeHundredType"/>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element minOccurs="0" name="PaidForThingsToDoJob" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="ExpensesToDoJob">
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element minOccurs="0" name="Expense" type="QuestionTextThreeHundredType"/>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element minOccurs="0" name="PaidForJobExpenses" type="QuestionTextType"/>
                                                                    <xs:element minOccurs="0" name="JobExpenses">
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element minOccurs="0" name="Expense" type="QuestionTextThreeHundredType"/>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element minOccurs="0" name="OtherEmployment" type="QuestionTextType"/>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="HavePartner" type="QuestionTextType"/>
                                            <xs:element minOccurs="0" name="Partner">
                                                <xs:complexType>
                                                    <xs:complexContent>
                                                        <xs:extension base="PersonType">
                                                            <xs:sequence>
                                                                <xs:element minOccurs="0" name="NationalityPartner" type="QuestionTextType"/>
                                                                <xs:element minOccurs="0" name="RelationshipStatus">
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element minOccurs="0" name="SeparatedFromPartner" type="QuestionTextType"/>
                                                                        </xs:sequence>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element minOccurs="0" name="IsCaree" type="QuestionTextType"/>
                                                            </xs:sequence>
                                                        </xs:extension>
                                                    </xs:complexContent>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element name="OtherBenefits">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="OtherMoneySSP" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="OtherMoneySSPDetails" type="StatutoryPay"/>
                                                        <xs:element minOccurs="0" name="OtherMoneySP" type="QuestionTextType">
                                                            <xs:annotation>
                                                                <xs:documentation>Statutory Maternity Pay (SMP), Statutory Paternity Pay (SPP) or Statutory Adoption Pay (SAP)</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:element>
                                                        <xs:element minOccurs="0" name="OtherMoneySPDetails" type="StatutoryPay"/>
                                                        <xs:element minOccurs="0" name="OtherMoney" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="OtherMoneyPayments" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="OtherMoneyDetails" nillable="false">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element minOccurs="0" name="Payment" type="PaymentWithFrequencyType"/>
                                                                    <xs:element minOccurs="0" name="Name" type="QuestionTextType"/>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                        <xs:element minOccurs="0" name="EEA">
                                                            <xs:annotation>
                                                                <xs:documentation>Applies to family as well and it is about EEA benefits</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:complexType>
                                                                <xs:all>
                                                                    <xs:element minOccurs="0" name="EEAReceivePensionsBenefits" type="QuestionTextType"> </xs:element>
                                                                    <xs:element minOccurs="0" name="EEAWorkingInsurance" type="QuestionTextType"/>
                                                                </xs:all>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="Payment">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="PaymentFrequency" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="InitialAccountQuestion" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" name="Account" type="AccountDetailsType"/>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="OtherInformation">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="WelshCommunication" type="QuestionTextType"/>
                                                        <xs:element minOccurs="0" maxOccurs="1" name="AdditionalInformation" type="QuestionYesNoWhyType"/>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element name="Declaration" type="DeclarationType" minOccurs="0"> </xs:element>
                                            <xs:element name="Disclaimer" type="DisclaimerType" minOccurs="0"> </xs:element>
                                            <xs:element name="EvidenceList" minOccurs="0" type="EvidenceListType"> </xs:element>
                                            <xs:element minOccurs="0" name="Consents" type="ConsentsType"/>
                                            <xs:element minOccurs="0" name="AssistedDecisions">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" maxOccurs="10" name="AssistedDecision" type="DecisionType"/>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="DWPCAChangeOfCircumstances">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element minOccurs="0" name="ClaimantDetails" type="CircsPersonType"> </xs:element>
                                            <xs:element minOccurs="0" name="CareeDetails" type="CircsPersonType"> </xs:element>
                                            <xs:element minOccurs="0" name="BreakFromCaring" type="BreakFromCaringChangeType"/>
                                            <xs:element minOccurs="0" name="StoppedCaring" type="StoppedCaringType"/>
                                            <xs:element minOccurs="0" name="AddressChange" type="AddressChangeType"/>
                                            <xs:element minOccurs="0" name="SelfEmployedChange" type="SelfEmployedChangeType"/>
                                            <xs:element minOccurs="0" name="PaymentChange" type="PaymentChangeType"/>
                                            <xs:element minOccurs="0" name="EmploymentChange" type="EmploymentChangeType"/>
                                            <xs:element minOccurs="0" name="OtherChanges" type="QuestionLongTextType"/>
                                            <xs:element minOccurs="0" name="Declaration" type="DeclarationType"> </xs:element>
                                            <xs:element minOccurs="0" name="EvidenceList" type="EvidenceListType"> </xs:element>
                                            <xs:element minOccurs="0" name="Consents" type="ConsentsType"/>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:choice>
                        </xs:sequence>
                        <xs:attribute name="id"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" ref="ds:Signature"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:simpleType name="RestrictedStringType">
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Za-zÀ-ƶ\s~0-9\(\)&amp;£€&quot;'!\+\-=_:;\.,@/\?]*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DisclaimerRestrictedStringType">
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Za-zÀ-ƶ\s~0-9\(\)&amp;£€&quot;'!\+\-=_:;\.,@/\?\[\]]*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LanguageType">
        <xs:annotation>
            <xs:documentation>A Common DataType Values from the ISO standard for 2 character language codes (ISO 639:1988 Code for the representation of names
                of languages). </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Za-z]{2}"/>
            <xs:enumeration value="en"/>
            <xs:enumeration value="cy"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AddresLineType">
        <xs:restriction base="RestrictedStringType">
            <xs:maxLength value="60"/>
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NationalInsuranceNumberType">
        <xs:restriction base="xs:string"> </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="QuestionNationalInsuranceNumberType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="NationalInsuranceNumberType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="PhoneNumberType">
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9 \-]{7,20}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="TextBlockType">
        <xs:sequence>
            <xs:element minOccurs="0" name="Title" type="RestrictedStringType"/>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Content" type="TwoThousandTextType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DisclaimerTextBlockType">
        <xs:sequence>
            <xs:element minOccurs="0" name="Title" type="DisclaimerRestrictedStringType"/>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Content" type="DisclaimerTwoThousandTextType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="UKPostalAddressType">
        <xs:sequence>
            <xs:element maxOccurs="5" name="Line" type="AddresLineType"/>
            <xs:element minOccurs="0" name="PostCode">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern
                                value="(GIR 0AA)|((([A-Z][0-9][0-9]?)|(([A-Z][A-HJ-Y][0-9][0-9]?)|(([A-Z][0-9][A-Z])|([A-Z][A-HJ-Y][0-9]?[A-Z])))) ?[0-9][A-Z]{2})"
                                />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="QuestionAddressType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="UKPostalAddressType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="QuestionPhoneNumberType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="PhoneNumberType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="QuestionType">
        <xs:sequence>
            <xs:element name="QuestionLabel">
                <xs:simpleType>
                    <xs:restriction base="RestrictedStringType">
                        <xs:maxLength value="240"/>
                        <xs:minLength value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="QuestionLongTextType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="TwoThousandTextType"> </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="QuestionDateType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="DateType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="QuestionDateTimeType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="DateTimeType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="QuestionTimeType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="TimeType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="QuestionTextType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="HundredTwentyTextType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="QuestionTextThreeHundredType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="ThreeHundredTextType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="QuestionSecureTextType">
        <xs:complexContent>
            <xs:extension base="QuestionType">
                <xs:sequence>
                    <xs:element name="Answer" type="HundredTwentyTextType"/>
                </xs:sequence>
            </xs:extension>
